class Mchildsurvey(models.Model):
    id = models.AutoField(primary_key=True)
    sampleid = models.CharField(max_length=7, unique=True, null=True, verbose_name=u"sampleid")
    march_id_fk = models.CharField(max_length=6, null=True, verbose_name=u"march_id_fk")
    ewcp_participant_id = models.CharField(max_length=15, blank=True, null=True, verbose_name=u"ewcp_participant_id")
    participation_level_id = models.CharField(max_length=2, blank=True, null=True, verbose_name=u"participation_level_id")
    echo_pin = models.CharField(max_length=3, blank=True, null=True, verbose_name=u"echo_pin")
    birth_date = models.DateField(blank=True, null=True, verbose_name=u"birth_date")
    # TODO: babdy six with varchar(1)?
    birth_sex = models.CharField(max_length=1, blank=True, null=True, verbose_name=u"birth_sex")

class Mmothersurvey(models.Model):
    id = models.AutoField(primary_key=True)
    march_id = models.CharField(max_length=6, unique=True, null=True, verbose_name=u"march_id")
    ewcp_participant_id = models.CharField(max_length=15, blank=True, null=True, verbose_name=u"ewcp_participant_id")
    participation_level_id = models.CharField(max_length=2, blank=True, null=True, verbose_name=u"participation_level_id")
    echo_pin = models.CharField(max_length=3, blank=True, null=True, verbose_name=u"echo_pin")
    specimen_id = models.CharField(max_length=4, blank=True, unique=True, null=True, verbose_name=u"specimen_id")

    mother_dob = models.DateField(blank=True, null=True, verbose_name=u"mother_dob")

    survey_outcome = Choices('Complete Interview', 'CHARM Q Only Complete', 'REDCap Only Complete', 'Partial Complete',
                             'NI, Reason Not Recorded', 'Refused Study Participation, Respondent',
                             'Final Refusal, Respondent', 'NI, Other Reason', 'NI, Locating Exhausted',
                             'NI, Deceased (Mom), Unable to Identify PCG', 'Unknown')
    # TODO: leave pn1 and pn2
    pn1_survey_outcome = models.CharField(max_length=200, blank=True, null=True, verbose_name=u"pn1_survey_outcome")
    pn2_survey_outcome = models.CharField(max_length=200, blank=True, null=True, verbose_name=u"pn2_survey_outcome")
    # TODO: pn3 choices follow arch - DONE
    pn3_survey_outcome = models.CharField(max_length=200, blank=True, choices=survey_outcome, null=True, verbose_name=u"pn3_survey_outcome")

    pn3_survey_date = models.DateField(blank=True, null=True, verbose_name=u"pn3_survey_date")
    pn3_giftcard_date = models.DateField(blank=True, null=True, verbose_name=u"pn3_giftcard_date")

    mother_first_name = models.CharField(max_length=50, blank=True, null=True, verbose_name=u"mother_first_name")
    mother_nickname = models.CharField(max_length=50, blank=True, null=True, verbose_name=u"mother_nickname")
    mother_last_name = models.CharField(max_length=50, blank=True, null=True, verbose_name=u"mother_last_name")
    mother_last_name_enrollment = models.CharField(max_length=200, blank=True, null=True, verbose_name=u"mother_last_name_enrollment")
    mother_maiden_name = models.CharField(max_length=50, blank=True, null=True, verbose_name=u"mother_maiden_name")

    # TODO: make it Yes/No - DONE
    choice_bool = Choices('Yes', 'No')
    texting = models.CharField(max_length=3, blank=True, choices=choice_bool, null=True, verbose_name=u"texting")
    recruitment_location = models.CharField(max_length=200, blank=True, null=True, verbose_name=u"recruitment_location")

    # TODO: leave consent version
    consent_version = models.CharField(max_length=200, blank=True, null=True, verbose_name=u"consent_version")

    consent_date = models.DateField(db_column='consent_date', blank=True, null=True, verbose_name=u"consent_date")
    due_date = models.DateField(db_column='due_date', blank=True, null=True, verbose_name=u"due_date")

